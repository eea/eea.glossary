<skos:prefLabel xml:lang="it">risorse idriche rinnovabili</skos:prefLabel>
<skos:definition></skos:definition>
</skos:Concept>
<skos:Concept rdf:about="http://www.semide.net/portal_thesaurus/Concept/120">
<skos:prefLabel xml:lang="en">renewable energy</skos:prefLabel>
<skos:prefLabel xml:lang="fr">énergie renouvelable</skos:prefLabel>
