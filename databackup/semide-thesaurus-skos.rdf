<skos:prefLabel xml:lang="fr">organisme national</skos:prefLabel>
<skos:prefLabel xml:lang="ar">مؤسسة وطنية</skos:prefLabel>
<skos:prefLabel xml:lang="it">organizzazione nazionale</skos:prefLabel>
<skos:prefLabel xml:lang="es">organismo nacional</skos:prefLabel>
<skos:definition></skos:definition>
</skos:Concept>
<skos:Concept rdf:about="http://www.semide.net/portal_thesaurus/Concept/565">
<skos:prefLabel xml:lang="en">environmental education</skos:prefLabel>
