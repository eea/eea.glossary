<skos:prefLabel xml:lang="es">zona tampón</skos:prefLabel>
<skos:definition></skos:definition>
</skos:Concept>
<skos:Concept rdf:about="http://www.semide.net/portal_thesaurus/Concept/673">
<skos:prefLabel xml:lang="en">protected area regime</skos:prefLabel>
<skos:prefLabel xml:lang="fr">registre des zones protégées</skos:prefLabel>
<skos:prefLabel xml:lang="ar">سجل كشف المناطق المحمية</skos:prefLabel>
<skos:prefLabel xml:lang="it">registro delle aree protette</skos:prefLabel>
<skos:prefLabel xml:lang="es">registro de zonas protegidas</skos:prefLabel>
<skos:definition></skos:definition>
</skos:Concept>
<skos:Concept rdf:about="http://www.semide.net/portal_thesaurus/Concept/674">
<skos:prefLabel xml:lang="en">protected area</skos:prefLabel>
<skos:prefLabel xml:lang="fr">zone protégée</skos:prefLabel>
<skos:prefLabel xml:lang="ar">منطقة محمية</skos:prefLabel>
<skos:prefLabel xml:lang="it">area protetta</skos:prefLabel>
<skos:prefLabel xml:lang="es">zona protegida</skos:prefLabel>
<skos:definition></skos:definition>
</skos:Concept>
