<skos:Concept rdf:about="http://www.semide.net/portal_thesaurus/Concept/509">
<skos:prefLabel xml:lang="en">lipid</skos:prefLabel>
<skos:prefLabel xml:lang="fr">lipide</skos:prefLabel>
