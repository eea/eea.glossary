<skos:prefLabel xml:lang="ar">إطماء</skos:prefLabel>
<skos:prefLabel xml:lang="it">interrimento</skos:prefLabel>
<skos:prefLabel xml:lang="es">atarquinamiento</skos:prefLabel>
