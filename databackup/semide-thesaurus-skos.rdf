<skos:prefLabel xml:lang="ar">طاقة حرارية</skos:prefLabel>
<skos:prefLabel xml:lang="it">energia termica</skos:prefLabel>
<skos:prefLabel xml:lang="es">energía térmica</skos:prefLabel>
<skos:definition></skos:definition>
</skos:Concept>
<skos:Concept rdf:about="http://www.semide.net/portal_thesaurus/Concept/128">
