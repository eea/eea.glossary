<skos:prefLabel xml:lang="en">flood wave</skos:prefLabel>
<skos:prefLabel xml:lang="fr">onde de crue</skos:prefLabel>
<skos:prefLabel xml:lang="ar">موجة الفيضان</skos:prefLabel>
<skos:prefLabel xml:lang="it">onda di piena</skos:prefLabel>
<skos:prefLabel xml:lang="es">onda de avenida</skos:prefLabel>
