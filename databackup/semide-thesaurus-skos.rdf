<skos:prefLabel xml:lang="it">invaso / accumulo</skos:prefLabel>
<skos:prefLabel xml:lang="es">pantano de montaña</skos:prefLabel>
<skos:definition></skos:definition>
</skos:Concept>
<skos:Concept rdf:about="http://www.semide.net/portal_thesaurus/Concept/550">
<skos:prefLabel xml:lang="en">Water governance</skos:prefLabel>
<skos:prefLabel xml:lang="fr">gouvernance de l&apos;eau</skos:prefLabel>
