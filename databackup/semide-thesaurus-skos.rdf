<skos:prefLabel xml:lang="it">autorità pubblica</skos:prefLabel>
<skos:prefLabel xml:lang="es">autoridad pública</skos:prefLabel>
<skos:definition></skos:definition>
</skos:Concept>
<skos:Concept rdf:about="http://www.semide.net/portal_thesaurus/Concept/561">
