<skos:prefLabel xml:lang="it">specie native</skos:prefLabel>
<skos:prefLabel xml:lang="lv">vietējā suga</skos:prefLabel>
<skos:prefLabel xml:lang="lt">vietinės rūšys</skos:prefLabel>
<skos:prefLabel xml:lang="no">stedegne arter</skos:prefLabel>
