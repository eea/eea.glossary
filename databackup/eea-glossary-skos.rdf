<skos:prefLabel xml:lang="en">genotoxicity</skos:prefLabel>
<skos:prefLabel xml:lang="et">genotoksilisus</skos:prefLabel>
<skos:prefLabel xml:lang="fi">genotoksisuus</skos:prefLabel>
<skos:prefLabel xml:lang="fr">génotoxicité</skos:prefLabel>
