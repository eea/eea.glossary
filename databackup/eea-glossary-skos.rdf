<skos:prefLabel xml:lang="pt">planície de inundação; planície de cheia</skos:prefLabel>
<skos:prefLabel xml:lang="ro">vale inundabilă</skos:prefLabel>
<skos:prefLabel xml:lang="sk">povodňová oblasť</skos:prefLabel>
<skos:prefLabel xml:lang="sl">poplavno območje</skos:prefLabel>
<skos:prefLabel xml:lang="es">llanura de inundación</skos:prefLabel>
<skos:prefLabel xml:lang="sv">översvämningsområde</skos:prefLabel>
<skos:prefLabel xml:lang="tr">taşkın alanı</skos:prefLabel>
