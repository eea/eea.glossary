<skos:prefLabel xml:lang="el">συμπύκνωση του εδάφους</skos:prefLabel>
<skos:prefLabel xml:lang="hu">{SOI} talajtömörödés</skos:prefLabel>
<skos:prefLabel xml:lang="is">jarðvegsþjöppun</skos:prefLabel>
