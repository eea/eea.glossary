<skos:prefLabel xml:lang="pt">impactes cumulativos</skos:prefLabel>
<skos:prefLabel xml:lang="ro">Impacturi cumulative</skos:prefLabel>
<skos:prefLabel xml:lang="sk">kumulatívne účinky</skos:prefLabel>
<skos:prefLabel xml:lang="sl">kumulativni učinki</skos:prefLabel>
<skos:prefLabel xml:lang="es">impacto acumulado</skos:prefLabel>
